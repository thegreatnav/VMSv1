<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:background="@drawable/my_border"
    android:focusable="auto">

    <TextView
        android:id="@+id/visitor_id_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Visitor ID:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/visitor_id_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:text="1"
        app:layout_constraintStart_toEndOf="@+id/visitor_id_text"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/visitor_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Visitor Name :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visitor_id_text" />

    <TextView
        android:id="@+id/visitor_name_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/visitor_name_text"
        app:layout_constraintTop_toBottomOf="@+id/mobile_number_text" />

    <TextView
        android:id="@+id/mobile_number_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="Mobile Number :"
        app:layout_constraintStart_toEndOf="@+id/visitor_id_textview"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mob_no_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/mobile_number_text"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/entry_date_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Entry Date:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/company_text" />

    <TextView
        android:id="@+id/entry_date_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/entry_date_text"
        app:layout_constraintTop_toBottomOf="@+id/company_textview" />

    <TextView
        android:id="@+id/designation_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:text="Designation :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visitor_name_text" />

    <TextView
        android:id="@+id/place_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:text="Place :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/designation_text" />

    <TextView
        android:id="@+id/company_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:text="Company :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/place_text" />

    <TextView
        android:id="@+id/visiting_staff_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:text="Visiting Staff :"
        app:layout_constraintEnd_toStartOf="@+id/visiting_staff_textview"
        app:layout_constraintTop_toBottomOf="@+id/mobile_number_text" />

    <TextView
        android:id="@+id/approver_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="10dp"
        android:text="Approver Name :"
        app:layout_constraintEnd_toStartOf="@+id/approver_name_textview"
        app:layout_constraintTop_toBottomOf="@+id/visiting_staff_text" />

    <TextView
        android:id="@+id/purpose_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="10dp"
        android:text="Purpose :"
        app:layout_constraintEnd_toStartOf="@+id/purpose_textview"
        app:layout_constraintTop_toBottomOf="@+id/approver_name_text" />

    <TextView
        android:id="@+id/visiting_area_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="10dp"
        android:text="Visiting Area :"
        app:layout_constraintEnd_toStartOf="@+id/visiting_area_textview"
        app:layout_constraintTop_toBottomOf="@+id/purpose_text" />

    <TextView
        android:id="@+id/exit_date_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:text="Exit Date :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/entry_date_text" />

    <TextView
        android:id="@+id/exit_date_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/exit_date_text"
        app:layout_constraintTop_toBottomOf="@+id/entry_date_textview" />

    <TextView
        android:id="@+id/visiting_staff_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/approver_name_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/visiting_staff_textview" />

    <TextView
        android:id="@+id/purpose_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/approver_name_textview" />

    <TextView
        android:id="@+id/visiting_area_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/purpose_textview" />

    <TextView
        android:id="@+id/designation_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/designation_text"
        app:layout_constraintTop_toBottomOf="@+id/visitor_name_textview" />

    <TextView
        android:id="@+id/place_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/place_text"
        app:layout_constraintTop_toBottomOf="@+id/designation_text" />

    <TextView
        android:id="@+id/company_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/company_text"
        app:layout_constraintTop_toBottomOf="@+id/place_textview" />

    <Button
        android:id="@+id/camera_status_button"
        android:layout_width="77dp"
        android:layout_height="67dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="20dp"
        android:text="Camera Status"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exit_date_text"
        app:toggleCheckedStateOnClick="true" />

    <Button
        android:id="@+id/nda_status_button"
        android:layout_width="77dp"
        android:layout_height="65dp"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="21dp"
        android:text="NDA Status"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/camera_status_button"
        app:layout_constraintTop_toTopOf="@+id/camera_status_button" />

    <Button
        android:id="@+id/exit_status_button"
        android:layout_width="77dp"
        android:layout_height="65dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="20dp"
        android:text="Exit Status"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/print_status_button" />

    <Button
        android:id="@+id/print_status_button"
        android:layout_width="77dp"
        android:layout_height="65dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="20dp"
        android:text="Print Status"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/exit_status_button"
        app:layout_constraintTop_toTopOf="@+id/nda_status_button" />
</androidx.constraintlayout.widget.ConstraintLayout>